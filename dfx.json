{
  "canisters": {
    "ledger": {
      "type": "custom",
      "candid": "src/ledger/ledger.did",
      "wasm": "src/ledger/ledger.wasm"
    },
    "georust":{
      "candid": "src/georust/georust.did",
      "package": "georust",
      "type": "rust"
    },
    "sustainations_dao": {
      "main": "src/sustainations_dao/main.mo",
      "type": "motoko",
      "dependencies": ["ledger", "georust"]
    },
    "internet_identity": {
      "type": "custom",
      "candid": "https://github.com/dfinity/internet-identity/releases/download/release-2023-01-16/internet_identity.did",
      "wasm": "https://github.com/dfinity/internet-identity/releases/download/release-2023-01-16/internet_identity_dev.wasm",
      "shrink": false,
      "remote": {
        "candid": "internet_identity.did",
        "id": {
          "ic": "rdmx6-jaaaa-aaaaa-aaadq-cai"
        }
      }
    },
    "basic_bitcoin": {
      "type": "custom",
      "candid": "src/basic_bitcoin/basic_bitcoin.did",
      "wasm": "src/basic_bitcoin/basic_bitcoin.wasm"
    },
    "frontend": {
      "dependencies": [
        "sustainations_dao"
      ],
      "frontend": {
        "entrypoint": "src/sustainations_dao_assets/src/index.html"
      },
      "source": [
        "dist/sustainations_dao_assets/"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": "vessel sources"
    },
    "replica": {
      "subnet_type":"system"
    }
  },
  "dfx": "0.12.1",
  "networks": {
    "local": {
      "bind": "127.0.0.1:8080",
      "type": "ephemeral"
    }
  },
  "version": 1
}